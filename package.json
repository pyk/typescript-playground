{
  "name": "typescript-playground",
  "version": "0.0.0",
  "description": "Bayu's TypeScript Playground",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "./node_modules/.bin/tsc"
  },
  "author": "Bayu Aldi Yansyah <bayualdiyansyah@gmail.com> (https://github.com/pyk)",
  "repository": "https://github.com/pyk/typescript-playground",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.3",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
